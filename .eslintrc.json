{
  "extends": [
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "rules": {
     "indent": ["error", 3],
     "react/react-in-jsx-scope": "off",
     "no-unused-vars": "warn",
     "object-curly-spacing": ["error", "always"],
     "react-hooks/exhaustive-deps": "off"
  }
}
